{
  "name": "@magnet.me/mm-on-enter-viewport",
  "version": "1.0.9",
  "description": "Directive to detect when an element has entered the viewport",
  "scripts": {
    "build-standalone": "npm run lint && webpack --progress --colors && gulp docs",
    "lint": "jshint src",
    "watch": "watch \"npm run build\" src",
    "copy-resources": "rsync -av --exclude=\"*.js\" src/* out",
    "build": "npm run lint && npm run copy-resources && babel src --out-dir out --source-maps inline --optional runtime,spec.protoToAssign",
    "clean": "rm -rf out docs",
    "prepublish": "npm run build",
    "test": "npm run build-standalone && karma start",
    "jenkins-test": "npm run build-standalone && karma start jenkins.karma.conf.js",
    "build-docs": "gulp docs",
    "live-docs": "gulp live-docs"
  },
  "main": "out/index.js",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-runtime": "^5.6.17",
    "lodash": "^2.4.1"
  },
  "peerDependencies": {
    "angular": "^1.3.0"
  },
  "devDependencies": {
    "angular": "^1.3.0",
    "angular-mocks": "^1.4.2",
    "babel": "^5.6.14",
    "babel-core": "^5.6.17",
    "babel-loader": "^5.3.1",
    "css-loader": "^0.15.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "gulp": "^3.9.0",
    "gulp-ngdocs": "^0.2.13",
    "gulp-util": "^3.0.6",
    "http-server": "^0.8.0",
    "jasmine-core": "^2.3.4",
    "jshint": "^2.8.0",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-html-reporter": "^0.1.8",
    "karma-junit-reporter": "^0.3.0",
    "karma-nested-reporter": "^0.1.3",
    "karma-ng-html2js-preprocessor": "^0.1.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^3.10.0",
    "@magnet.me/magnet-test-utils": "^1.0.0",
    "node-libs-browser": "^0.5.2",
    "phantomjs": "^1.9.17",
    "style-loader": "^0.12.3",
    "watch": "^0.16.0",
    "webpack": "^1.10.1"
  }
}
